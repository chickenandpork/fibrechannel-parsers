package org.smallfoot.parser.zone;

public abstract class ZoneParser extends Thread
{
    public abstract
    java.util.Enumeration<ZPZoneEntry> zoneElements();
    public abstract
    int zoneSize();
    public abstract
    java.util.Enumeration<ZPAliasEntry> aliasElements();
    public abstract ZPAliasEntry[] aliasArray();
    public abstract
    int aliasSize();
    public abstract
    void setReader(java.io.Reader is);
}
